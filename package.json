{
    "name": "obsidian-satset-sync",
    "version": "1.0.2",
    "description": "Satset Sync plugin for Obsidian",
    "main": "main.js",
    "scripts": {
        "build": "esbuild src/main.ts --bundle --external:obsidian --outfile=main.js --format=cjs --platform=node --target=es2018",
